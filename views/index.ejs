  <body>
    <main id="log-reg-container">
      <h1 class="restaurant-journal">Restaurant Journal</h1>
      <div class="card">
        <div class="card-content" id="log-reg-card">
          <div class="content">
            <form action="/register">
              <p>Need to Sign Up?</p>
              <button class="button is-success">Register</button>
            </form>
          </div>
          <div class="content">
            <form action="/login">
              <p>Already have an account?</p>
              <button class="button is-link">Login</button>
            </form>
          </div>
        </div>
      </div>
    </main>
  </body>
